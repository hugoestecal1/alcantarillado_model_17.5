FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    boxToCell
    {
        box (0.45 0.42 0) (-0.42 -0.42 0.190);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);


